{
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "ignorePatterns": [
    "src/components/**"
  ],
  "rules": {
    "prettier/prettier": [
      "error",
      {
        "indent": "off",
        "singleQuote": false
      }
    ],
    "indent": "off"
  }
}